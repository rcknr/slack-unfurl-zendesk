{
    "name": "rcknr/slack-unfurl-zendesk",
    "description": "Zendesk links unfurler for slack-unfurl",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ZendeskSlackUnfurl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendeskSlackUnfurl\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-json": "*"
    },
    "require-dev": {
        "donatj/mock-webserver": "v2.0.0",
        "glen/slack-unfurl": "~0.4.0",
        "phpunit/phpunit": "^6.5",
        "symfony/var-dumper": "^4.0"
    }
}
